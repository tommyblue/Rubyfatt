<%= content_for :breadcrumb do %>
  <%= print_breadcrumbs([
    ["Wiki", wiki_page_path],
    [t('breadcrumb.wiki.all_pages', default: "All pages"), nil]
  ]).html_safe %>
<% end %>

<h1><%=wt 'All pages' %></h1>

<ul>
  <% wiki_paginate @pages do -%>
    <% @pages.each do |page| -%>
      <li><%= link_to page.title, wiki_page_path( page ) %></li>
    <% end -%>
  <% end -%>
</ul>
